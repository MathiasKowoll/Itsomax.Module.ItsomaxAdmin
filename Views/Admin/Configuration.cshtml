@using Itsomax.Module.Core.Services
@using Itsomax.Module.Core.Models
@model IList<AppSetting>
@inject GetSettings GetSettings
<div class="container-fluid">
    <br />
    <div class="row">
        <div class="col-lg-12">
            <div class="card card-outline-info">
                <div class="card-header bg-theme">
                    <h2 class="m-b-0 text-white">System configuration for @GetSettings.SystemTitle()</h2>
                </div>
                <div class="card-body">
                    <form autocomplete="off" class="form-horizontal p-t-20" asp-controller="Admin" asp-action="Configuration" method="post">
                        
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>